<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="actionbar_background">#FFFFFF</color>
    <color name="actionbar_text">#535391</color>
    <color name="app_background">#FFFFFF</color>
    <color name="app_text">#535391</color>
    <color name="black">#000000</color>
    <color name="black_overlay">#66000000</color>
    <color name="blue">#22A7F0</color>
    <color name="button_background">#535391</color>
    <color name="button_text">#FFFFFF</color>
    <color name="chat_text">#535391</color>
    <color name="chat_text_background">#90E5F2</color>
    <color name="chat_text_background_mine">#7980DB</color>
    <color name="gray">#DADFE1</color>
    <color name="green">#26C281</color>
    <color name="purple">#8E44AD</color>
    <color name="red">#F22613</color>
    <color name="white">#FFFFFF</color>
    <color name="yellow">#F4D03F</color>
    <declare-styleable name="ButtonBarContainerTheme"><attr format="reference" name="metaButtonBarStyle"/><attr format="reference" name="metaButtonBarButtonStyle"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="large_brush">30dp</dimen>
    <dimen name="medium_brush">20dp</dimen>
    <dimen name="small_brush">10dp</dimen>
    <integer name="large_size">30</integer>
    <integer name="medium_size">20</integer>
    <integer name="small_size">10</integer>
    <string name="action_settings">Settings</string>
    <string name="app_name">GoWifi</string>
    <string name="back_button">Back</string>
    <string name="disconnect">Disconnect</string>
    <string name="download_image">Download image</string>
    <string name="enter_text">Enter your message here</string>
    <string name="file_icon">File icon</string>
    <string name="go_to_settings_button">Go to Settings</string>
    <string name="go_to_settings_text">Click to Settings
         and connect your device to the others via Wi-Fi Direct</string>
    <string name="pick_image">Pick image</string>
    <string name="send_button">Send</string>
    <string name="send_drawing">Send drawing</string>
    <string name="send_file">Send file</string>
    <string name="send_image">Send image</string>
    <string name="set_chat_name">Please choose your chat name</string>
    <string name="take_photo">Take photo</string>
    <string name="title_activity_file_picker">Choose the file</string>
    <string name="title_activity_view_image">View image</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="ButtonBar">
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:background">@android:drawable/bottom_bar</item>
    </style>
    <style name="ButtonBarButton"/>
    <style name="CustomActionBarTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>   
        <item name="android:windowContentOverlay">@null</item>     
    </style>
    <style name="FullscreenTheme" parent="android:Theme.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@null</item>
        <item name="metaButtonBarStyle">@style/ButtonBar</item>
        <item name="metaButtonBarButtonStyle">@style/ButtonBarButton</item>
    </style>
    <style name="MyActionBar" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@color/actionbar_background</item>
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>
        <item name="android:actionOverflowButtonStyle">@style/OverflowStyle</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="MyActionBarTitleText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@color/actionbar_text</item>
    </style>
    <style name="OverflowStyle" parent="@android:style/Widget.Holo.ActionButton.Overflow">
	</style>
</resources>